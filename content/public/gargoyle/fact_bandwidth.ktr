        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here


tmp    = substr(bytes, 0, dl_cmd.length );
ul_tmp = substr(bytes, 0, ul_cmd.length );

if ( tmp == dl_cmd )
{
 ip_addr = rtrim(substr(bytes, dl_cmd.length+1));
 bw_type = 'download';
 bytes =0;
}
else {
	if (ul_tmp == ul_cmd) {
		ip_addr = rtrim(substr(bytes, ul_cmd.length+1));
        bw_type = 'upload';
	bytes = 0;
	}
	else {
	kbyte  = bytes/1024;
	bytes  = kbyte*1024;
	startd =  str2date(start_date,"E MMM dd HH:mm:ss yyyy")
	endd   =  str2date(end_date,"E MMM dd HH:mm:ss yyyy")
	}

}

</jsScript_script>
