, t3.quota_owner ||':'|| t3.quota || ':'|| quota_pct_used as mail_message
FROM "public".f_quota_event t1
INNER JOIN public.dim_date t2 ON t2.date_key = t1.date_key and t2."DATE" >= to_timestamp('${file_date}', 'YYYY-MM-DD-HH24-MI-SS')
