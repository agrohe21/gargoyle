    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>try {
	twold = stdOut.split(".");
	twold = twold[twold.length-2]+"."+twold[twold.length-1]
}catch(e) {
	twold = 'unknown';
}

try {
	tld = stdOut.split(".");
	tld = tld[tld.length-3]+"."+tld[tld.length-2]+"."+tld[tld.length-1]
}catch(e) {
	tld = 'unknown';
}

try {
	top = stdOut.split(".");
	top = top[top.length-1]
}catch(e) {
	top = 'unknown';
}


//hostname = getHostName(stdOut);
domain   = getDomain(stdOut);</jsScript_script>
